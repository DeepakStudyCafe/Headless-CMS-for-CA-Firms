{
  "name": "headless-cms-system",
  "version": "1.0.0",
  "description": "Complete Headless CMS for managing CA firm websites",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:admin && npm run install:firm1 && npm run install:firm2 && npm run install:firm3 && npm run install:firm4 && npm run install:firm5 && npm run install:firm6",
    "install:backend": "cd backend && npm install",
    "install:admin": "cd admin-dashboard && npm install",
    "install:firm1": "cd websites/firm1-sharma && npm install",
    "install:firm2": "cd websites/firm2-verma && npm install",
    "install:firm3": "cd websites/firm3-gupta && npm install",
    "install:firm4": "cd websites/firm4-kapoor && npm install",
    "install:firm5": "cd websites/firm5-singh && npm install",
    "install:firm6": "cd websites/firm6-patel && npm install",
    "build:all": "npm run build:backend && npm run build:admin && npm run build:firm1 && npm run build:firm2 && npm run build:firm3 && npm run build:firm4 && npm run build:firm5 && npm run build:firm6",
    "build:backend": "cd backend && npm run build",
    "build:admin": "cd admin-dashboard && npm run build",
    "build:firm1": "cd websites/firm1-sharma && npm run build",
    "build:firm2": "cd websites/firm2-verma && npm run build",
    "build:firm3": "cd websites/firm3-gupta && npm run build",
    "build:firm4": "cd websites/firm4-kapoor && npm run build",
    "build:firm5": "cd websites/firm5-singh && npm run build",
    "build:firm6": "cd websites/firm6-patel && npm run build",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:admin\" \"npm run start:firm1\" \"npm run start:firm2\" \"npm run start:firm3\" \"npm run start:firm4\" \"npm run start:firm5\" \"npm run start:firm6\"",
    "start:backend": "cd backend && npm run start",
    "start:admin": "cd admin-dashboard && npm run start",
    "start:firm1": "cd websites/firm1-sharma && npm run start",
    "start:firm2": "cd websites/firm2-verma && npm run start",
    "start:firm3": "cd websites/firm3-gupta && npm run start",
    "start:firm4": "cd websites/firm4-kapoor && npm run start",
    "start:firm5": "cd websites/firm5-singh && npm run start",
    "start:firm6": "cd websites/firm6-patel && npm run start",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:admin\" \"npm run dev:firm1\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:admin": "cd admin-dashboard && npm run dev",
    "dev:firm1": "cd websites/firm1-sharma && npm run dev",
    "db:migrate": "cd backend && npm run prisma:migrate",
    "db:seed": "cd backend && npm run seed",
    "db:studio": "cd backend && npm run prisma:studio"
  },
  "keywords": [
    "cms",
    "headless",
    "nextjs",
    "nodejs",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "express": "^5.2.1",
    "next": "^16.1.6",
    "rss-parser": "^3.13.0"
  }
}
